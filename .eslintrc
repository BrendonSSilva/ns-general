{
  "extends": "plugin:nullstack/recommended",
  "rules": {
    "nullstack/prettier": [
      "warn",
      {
        "trailingComma": "all",
        "tabWidth": 0,
        "semi": false,
        "singleQuote": true,
        "printWidth": 120
      },
      "error",
      {
        "singleQuote": true,
        "parser": "flow"
      },
      {
        "usePrettierrc": false
      }
    ]
  }
}